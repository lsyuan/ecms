<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title></title>
    <link href="@Url.Content("~/Content/css/EasyUICss/easyui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/css/EasyUICss/icon.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery.autocomplete.css")" rel="stylesheet" type="text/css" />
    <!--jqueryEasyUI-->
    <script src="@Url.Content("~/Scripts/rounded_corners_lite.inc.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.easyui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/json2.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/easyui-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Ajax.Common.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.autocomplete.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.jbind.js")"  charset="utf-8" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.JsonData.js")"  charset="utf-8" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/js/CommonJS.js")" type="text/javascript"></script>
    @*<script src="@Url.Content("~/Scripts/jquery-ui.js")"></script>*@
    <script type="text/javascript">
        $(function () {
            settings = {
                tl: { radius: 5 },
                tr: { radius: 5 },
                bl: { radius: 5 },
                br: { radius: 5 },
                antiAlias: true,
                autoPad: false
            }
            var myBoxObject = new curvyCorners(settings, "layoutDiv");
            myBoxObject.applyCornersToAll();
        });
    </script>
    <style type="text/css">
        #layoutDiv { width: 100%; color: #3960A2; background-color: #8eb7fb; font-family: 微软雅黑; }
        #titleDiv { border: 0px solid blue; width: 100%; height: 35px; line-height: 35px; font-size: 15px; }
            #titleDiv img { margin-left: 5px; vertical-align: middle; }
        #contentDiv { border-top: 1px solid #4cace5; border-bottom: 1px solid #4cace5; padding: 5px; padding-left: 0px; padding-right: 0px; background-color: White; margin: 0 auto; margin-left: 1px; margin-right: 1px; }
        #dgRequired ul { padding: 0px; margin: 0px; list-style: none; }
            #dgRequired ul li { display: block; font-weight: bolder; margin-bottom: 3px; cursor: pointer; }
                #dgRequired ul li:hover { font-size: 14px; line-height: 14px; }
    </style>
    @RenderSection("head", required: false)
</head>
<body style="padding: 0px;">
    <div id="layoutDiv" class="layoutDiv">
        <div id="titleDiv" class="titleDiv">
            <img src="../../Content/images/window.png" alt="tipIcon" style="width: 24px; height: 24px;" />
            @ViewBag.Title
        </div>
        <div id="contentDiv">@RenderBody()</div>
    </div>
</body>
</html>
